<table class="ui table">
  <thead>
    <tr>
      <th>Song</th>
      <th>Artist</th>
      <th>Duration</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each playlist.songs}}
      <tr>
        <td>
          {{title}}
        </td>
        <td>
          {{artist}}
        </td>
        <td>
          {{duration}}
        </td>
        <td>
          {{genre}}
        </td>
         <td>
          <a href="/playlist/{{../playlist.id}}/deletesong/{{id}}" class="ui icon button"><i class="trash icon"></i></a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>